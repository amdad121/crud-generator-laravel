{
    "name": "amdadulhaq/crud-generator-laravel",
    "description": "A package to generate CRUD operations easily.",
    "authors": [
        {
            "name": "Amdadul Haq",
            "email": "amdadulhaq781@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "AmdadulHaq\\CRUDGenerator\\": "src/"
        }
    },
    "scripts": {
        "lint": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AmdadulHaq\\CRUDGenerator\\CrudServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
